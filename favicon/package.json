{
  "name": "@dnb-hugo/favicon",
  "description": "Favicons for Hugo",
  "version": "1.0.12",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-hugo/components/issues"
  },
  "commitlint": {
    "extends": [
      "@dnb-hugo/commitlint-config"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@dnb-hugo/browserslist-config": "github:dnb-hugo/browserslist-config",
    "@dnb-hugo/commitlint-config": "github:dnb-hugo/commitlint-config",
    "@dnb-hugo/eslint-config": "github:dnb-hugo/eslint-config",
    "@dnb-hugo/remark-preset-lint": "github:dnb-hugo/remark-preset-lint",
    "@dnb-hugo/standard-version-config": "github:dnb-hugo/standard-version-config",
    "@dnb-hugo/stylelint-config": "github:dnb-hugo/stylelint-config",
    "@dnb-hugo/textlint-config": "github:dnb-hugo/textlint-config",
    "husky": "4.3.8",
    "standard-version": "9.1.0"
  },
  "homepage": "https://github.com/dnb-hugo/components/tree/master/favicon",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "commitlint": "commitlint --from=HEAD~1",
    "release": "standard-version --release-as patch -a -t \"favicon/v\" --releaseCommitMessageFormat \"chore(release): favicon/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"favicon/v\" --releaseCommitMessageFormat \"chore(release): favicon/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"favicon/v\" --releaseCommitMessageFormat \"chore(release): favicon/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    },
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  }
}
