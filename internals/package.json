{
  "name": "@dnb-org-gohugo-components/internals",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Better internal templates for GoHugo",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "homepage": "https://github.com/dnb-org/components/tree/master/internals",
  "bugs": "https://github.com/dnb-org/components/issues",
  "devDependencies": {
    "@dnb-org/standard-version-config": "3.3.7"
  },
  "scripts": {
    "release": "npm run release:patch",
    "release:major": "standard-version --release-as major -a -t \"internals/v\" --releaseCommitMessageFormat \"chore(release): internals/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:minor": "standard-version --release-as minor -a -t \"internals/v\" --releaseCommitMessageFormat \"chore(release): internals/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:patch": "standard-version --release-as patch -a -t \"internals/v\" --releaseCommitMessageFormat \"chore(release): internals/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  },
  "private": true
}
