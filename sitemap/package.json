{
  "name": "@dnb-org-components/sitemap",
  "version": "1.0.8",
  "description": "A sitemap for Hugo",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "homepage": "https://github.com/dnb-org/components/tree/master/sitemap",
  "license": "MIT",
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"sitemap/v\" --releaseCommitMessageFormat \"chore(release): sitemap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"sitemap/v\" --releaseCommitMessageFormat \"chore(release): sitemap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"sitemap/v\" --releaseCommitMessageFormat \"chore(release): sitemap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  },
  "bugs": {
    "url": "https://github.com/dnb-org/components/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@dnb-org/browserslist-config": "3.2.23",
    "@dnb-org/commitlint-config": "3.2.22",
    "@dnb-org/eslint-config": "3.2.22",
    "@dnb-org/remark-config": "3.2.25",
    "@dnb-org/standard-version-config": "3.2.22",
    "@dnb-org/stylelint-config": "3.3.0",
    "@dnb-org/textlint-config": "3.2.22",
    "standard-version": "9.3.1"
  },
  "commitlint": {
    "extends": [
      "@dnb-org/commitlint-config"
    ]
  },
  "private": true
}
