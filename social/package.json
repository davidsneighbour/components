{
  "name": "@dnb-org/social",
  "description": "Social Media support for Hugo",
  "version": "0.0.9",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-org/components/issues"
  },
  "commitlint": {
    "extends": [
      "@dnb-org/commitlint-config"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@dnb-org/browserslist-config": "3.2.20",
    "@dnb-org/commitlint-config": "3.2.22",
    "@dnb-org/eslint-config": "3.2.19",
    "@dnb-org/remark-config": "3.2.18",
    "@dnb-org/standard-version-config": "3.2.8",
    "@dnb-org/stylelint-config": "3.2.8",
    "@dnb-org/textlint-config": "3.2.8",
    "standard-version": "9.3.1"
  },
  "homepage": "https://github.com/dnb-org/components/tree/master/social",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"social/v\" --releaseCommitMessageFormat \"chore(release): sitemap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"social/v\" --releaseCommitMessageFormat \"chore(release): sitemap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"social/v\" --releaseCommitMessageFormat \"chore(release): sitemap/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  }
}
